{
  "version": 3,
  "builds": [
    {
      "src": "backend/dist/src/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/dist/src/main.js"
    }
  ],
  "functions": {
    "backend/dist/src/main.js": {
      "runtime": "nodejs20.x",
      "maxDuration": 10
    }
  }
}
